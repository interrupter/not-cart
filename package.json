{
		"name": "not-cart",
		"version": "0.0.18",
		"description": "not-* family cart model and interface in not- environment",
		"main": "index.js",
		"scripts": {
				"build": "./bin/build.sh",
				"buildtest": "ENV=test ./bin/build.sh && rm -rf ./test/browser/assets/cart/* && mv dist/* ./test/browser/assets/cart/",
				"prelint": "npm run build",
				"lint": "./node_modules/.bin/eslint ./src/**/**/*.js",
				"cypress:run": "NODE_ENV=test TEST_TYPE=run node ./test/cypress.env.js",
				"cypress:open": "NODE_ENV=test TEST_TYPE=open node ./test/cypress.env.js",
				"test:open": "_ENV_NODE_='liquid' ./node_modules/.bin/mocha --reporter spec ./test/node && npm run cypress:open",
				"test:run": "_ENV_NODE_='liquid' ./node_modules/.bin/mocha --reporter spec ./test/node && npm run cypress:run",
				"docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
				"cover": "_ENV_NODE_='liquid' nyc npm run test"
		},
		"repository": {
				"type": "git",
				"url": "git+https://github.com/interrupter/not-cart.git"
		},
		"keywords": [
				"not-framework",
				"not-node",
				"cart"
		],
		"author": "interrupter <alex.repin@mail.ru> (http://github.com/interrupter)",
		"license": "ISC",
		"bugs": {
				"url": "https://github.com/interrupter/not-user/issues"
		},
		"homepage": "https://github.com/interrupter/not-user#readme",
		"dependencies": {
				"not-bulma": "^0.2.10",
				"not-config": "*",
				"not-error": "*",
				"not-filter": "*",
				"not-locale": "*",
				"not-log": "*",
				"not-node": "*",
				"not-one-time-code": "*",
				"rollup": "^2.32.0",
				"sass": "^1.27.0",
				"uuidv4": "*",
				"validator": "^13.1.17"
		},
		"devDependencies": {
				"@babel/core": "^7.12.3",
				"@babel/plugin-transform-arrow-functions": "^7.12.1",
				"@babel/plugin-transform-runtime": "^7.12.1",
				"@babel/preset-env": "^7.12.1",
				"@cypress/code-coverage": "^3.8.2",
				"babel-jest": "^26.5.2",
				"babel-plugin-istanbul": "^6.0.0",
				"babel-preset-env": "^1.7.0",
				"chai": "^4.2.0",
				"chai-as-promised": "*",
				"connect": "^3.7.0",
				"cypress": "^5.4.0",
				"electron": "^10.1.3",
				"electron-mocha": "^9.2.0",
				"eslint": "^7.11.0",
				"expect": "^26.5.3",
				"ink-docstrap": "*",
				"istanbul-lib-coverage": "^3.0.0",
				"jsdoc": "^3.6.6",
				"jsdom": "^16.4.0",
				"jsdom-global": "^3.0.2",
				"mocha": "^8.2.0",
				"mocha-webpack": "^1.1.0",
				"mongodb-memory-server": "^6.9.2",
				"not-overlay": "*",
				"nyc": "^15.1.0",
				"regenerator-runtime": "^0.13.7",
				"request": "^2.88.2",
				"retire": "^2.2.3",
				"rollup-plugin-babel": "^4.4.0",
				"rollup-plugin-commonjs": "^10.1.0",
				"rollup-plugin-istanbul": "^2.0.1",
				"rollup-plugin-node-resolve": "^5.2.0",
				"rollup-plugin-postcss": "^3.1.8",
				"rollup-plugin-sass": "^1.2.2",
				"rollup-plugin-svelte": "^6.0.1",
				"serve-static": "^1.14.1",
				"should": "^13.2.3",
				"svelte": "^3.29.0",
				"whatwg-fetch": "^3.4.1"
		},
		"nyc": {
				"include": [
						"index.js",
						"src/**/**/**.js"
				],
				"exclude": [
						"test/**/**/**/**.js",
						"node_modules/"
				],
				"reporter": [
						"html"
				],
				"check-coverage": false,
				"per-file": false,
				"lines": 95,
				"statements": 95,
				"functions": 95,
				"branches": 95,
				"watermarks": {
						"lines": [
								80,
								95
						],
						"functions": [
								80,
								95
						],
						"branches": [
								80,
								95
						],
						"statements": [
								80,
								95
						]
				}
		}
}
