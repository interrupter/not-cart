{
		"name": "not-cart",
		"version": "0.0.1",
		"description": "not-* family cart model and interface in not- environment",
		"main": "index.js",
		"scripts": {
				"build": "./bin/build.sh",
				"buildtest": "ENV=test ./bin/build.sh",
				"prelint": "npm run build",
				"lint": "./node_modules/.bin/eslint ./src/**/**/*.js",
				"pretest": "npm run buildtest",
				"cypress": "NODE_ENV=test node ./test/cypress.env.js",
				"test": "_ENV_NODE_='liquid' ./node_modules/.bin/mocha --reporter spec ./test/node && npm run cypress",
				"docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
				"cover": "_ENV_NODE_='liquid' nyc npm run test"
		},
		"repository": {
				"type": "git",
				"url": "git+https://github.com/interrupter/not-cart.git"
		},
		"keywords": [
				"not-framework",
				"not-node",
				"cart"
		],
		"author": "interrupter <alex.repin@mail.ru> (http://github.com/interrupter)",
		"license": "ISC",
		"bugs": {
				"url": "https://github.com/interrupter/not-user/issues"
		},
		"homepage": "https://github.com/interrupter/not-user#readme",
		"dependencies": {
				"not-config": "0.0.4",
				"not-error": "0.0.20",
				"not-filter": "^0.2.2",
				"not-locale": "*",
				"not-log": "0.0.14",
				"not-node": "^3.3.47",
				"not-one-time-code": "^0.1.1",
				"uuidv4": "*",
				"validator": "^13.0.0"
		},
		"devDependencies": {
				"@babel/core": "^7.9.0",
				"@babel/plugin-transform-arrow-functions": "^7.8.3",
				"@babel/plugin-transform-runtime": "^7.9.0",
				"@babel/preset-env": "^7.9.0",
				"@cypress/code-coverage": "^1.14.0",
				"babel-jest": "^25.1.0",
				"babel-plugin-istanbul": "^6.0.0",
				"babel-preset-env": "^1.7.0",
				"chai": "^4.2.0",
				"chai-as-promised": "*",
				"connect": "^3.7.0",
				"cypress": "^4.2.0",
				"electron": "^8.1.1",
				"electron-mocha": "^8.2.1",
				"eslint": "^6.8.0",
				"expect": "^25.1.0",
				"ink-docstrap": "*",
				"istanbul-lib-coverage": "^3.0.0",
				"jest": "^25.1.0",
				"jsdoc": "^3.6.3",
				"jsdom": "^16.2.1",
				"jsdom-global": "^3.0.2",
				"mocha": "^7.1.1",
				"mocha-webpack": "^1.1.0",
				"mongodb-memory-server": "^6.4.1",
				"ncp": "^2.0.0",
				"nyc": "^15.0.0",
				"regenerator-runtime": "^0.13.5",
				"request": "^2.88.2",
				"retire": "^2.1.2",
				"rollup-plugin-babel": "^4.4.0",
				"rollup-plugin-commonjs": "^10.1.0",
				"rollup-plugin-istanbul": "^2.0.1",
				"rollup-plugin-node-resolve": "^5.2.0",
				"serve-static": "^1.14.1",
				"should": "^13.2.3",
				"whatwg-fetch": "^3.0.0"
		},
		"nyc": {
				"include": [
						"index.js",
						"src/**/**/**.js"
				],
				"exclude": [
						"test/**/**/**/**.js",
						"node_modules/"
				],
				"reporter": [
						"html"
				],
				"check-coverage": false,
				"per-file": false,
				"lines": 95,
				"statements": 95,
				"functions": 95,
				"branches": 95,
				"watermarks": {
						"lines": [
								80,
								95
						],
						"functions": [
								80,
								95
						],
						"branches": [
								80,
								95
						],
						"statements": [
								80,
								95
						]
				}
		}
}
